{
  "private": true,
  "version": "0.1.0",
  "name": "svelte-ssr-worker",
  "scripts": {
    "build": "run-s build:**",
    "build:dom": "rollup -c config/dom.js",
    "build:ssr": "rollup -c config/service-worker.js",
    "start": "sirv public --single --dev --port 8080",
    "watch": "run-p \"start --quiet\" watch:dom",
    "watch:dom": "run-s \"build:dom -w\"",
    "watch:ssr": "rollup -cw config/service-worker.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "npm-run-all": "4.1.5",
    "rollup": "2.28.2",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-string": "3.0.0",
    "rollup-plugin-svelte": "6.0.1",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.29.0"
  }
}
